{
    "name": "ng2-dropdown-treeview",
    "version": "0.0.1",
    "description": "An Angular 2 checkable dropdown treeview component with multiple selection",
    "scripts": {
        "sass": "node-sass src -o src --output-style=expanded --indent-width=4",
        "prepublish": "npm run build:ngc && npm run build:lib",
        "build:ngc": "ngc -p tsconfig-ngc.json",
        "build:lib": "webpack",
        "build:dev": "webpack-dev-server --inline --progress --port 12346 --open --config demo/config/webpack.dev.js",
        "build:prod": "webpack --config demo/config/webpack.prod.js --progress --profile --bail"
    },
    "keywords": [
        "ng2",
        "angular",
        "angular2",
        "angular 2",
        "dropdown",
        "treeview",
        "tree",
        "multiselect",
        "multiple selection",
        "multiple level",
        "checkable",
        "checkbox",
        "collapse",
        "expand"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/leovo2708/ng2-dropdown-treeview.git"
    },
    "author": "Leo Vo",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/leovo2708/ng2-dropdown-treeview/issues"
    },
    "main": "bundles/ng2-dropdown-treeview.js",
    "module": "index.js",
    "typings": "index.d.ts",
    "homepage": "https://github.com/leovo2708/ng2-dropdown-treeview",
    "peerDependencies": {
        "@angular/common": "^2.3.1",
        "@angular/core": "^2.3.1",
        "@angular/forms": "^2.3.1",
        "lodash": "^4.17.2"
    },
    "devDependencies": {
        "@angular/common": "^2.3.1",
        "@angular/compiler": "^2.3.1",
        "@angular/compiler-cli": "^2.3.1",
        "@angular/core": "^2.3.1",
        "@angular/forms": "^2.3.1",
        "@angular/platform-browser": "^2.3.1",
        "@angular/platform-browser-dynamic": "^2.3.1",
        "@types/core-js": "^0.9.35",
        "@types/jasmine": "^2.5.38",
        "@types/karma-fixture": "^0.2.2",
        "@types/lodash": "^4.14.43",
        "@types/node": "^6.0.52",
        "angular2-template-loader": "^0.6.0",
        "awesome-typescript-loader": "^3.0.0-beta.17",
        "bootstrap": "^4.0.0-alpha.5",
        "clean-webpack-plugin": "^0.1.14",
        "codelyzer": "^2.0.0-beta.3",
        "core-js": "^2.4.1",
        "css-loader": "^0.26.1",
        "ejs-loader": "^0.3.0",
        "extract-text-webpack-plugin": "^2.0.0-beta.4",
        "file-loader": "^0.9.0",
        "font-awesome": "^4.7.0",
        "html-webpack-plugin": "^2.24.1",
        "lodash": "^4.17.2",
        "node-sass": "^4.1.0",
        "optimize-css-assets-webpack-plugin": "^1.3.0",
        "raw-loader": "^0.5.1",
        "rxjs": "^5.0.1",
        "sass-loader": "^4.1.0",
        "style-loader": "^0.13.1",
        "tslint": "^4.1.1",
        "tslint-loader": "^3.3.0",
        "typescript": "2.0.10",
        "url-loader": "^0.5.7",
        "webpack": "^2.2.0-rc.1",
        "webpack-dev-server": "^2.2.0-rc.0",
        "webpack-merge": "^1.1.1",
        "zone.js": "^0.7.3"
